package chap17_generic.clazz;

public class BoardFile<T> {
	private int boardNo;
	private int tytle;
	private String boardFileNm;
	
	private T t;

	public int getBoardNo() {
		return boardNo;
	}

	public void setBoardNo(int boardNo) {
		this.boardNo = boardNo;
	}

	public int getTytle() {
		return tytle;
	}

	public void setTytle(int tytle) {
		this.tytle = tytle;
	}

	public String getBoardFileNm() {
		return boardFileNm;
	}

	public void setBoardFileNm(String boardFileNm) {
		this.boardFileNm = boardFileNm;
	}

	public T getT() {
		return t;
	}

	public void setT(T t) {
		this.t = t;
	}

	@Override
	public String toString() {
		return "BoardFile [boardNo=" + boardNo + ", tytle=" + tytle + ", boardFileNm=" + boardFileNm + ", t=" + t + "]";
	}
}
